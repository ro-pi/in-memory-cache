{
  "name": "ropi/in-memory-cache",
  "type": "library",
  "description": "PSR-16 compatible in-memory cache library",
  "keywords": [
    "in-memory",
    "memory",
    "cache",
    "psr-16",
    "psr16"
  ],
  "homepage": "https://www.robertpiplica.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Piplica",
      "email": "mail@robertpiplica.de"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "psr/simple-cache": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Ropi\\InMemoryCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ropi\\InMemoryCache\\Tests\\": "tests/"
    }
  }
}
